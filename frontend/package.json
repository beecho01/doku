{
  "name": "doku-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:frontend": "vite",
    "dev:backend": "cd ../app && python main.py",
    "dev:full": "concurrently \"bun run dev:frontend\" \"bun run dev:backend\"",
    "build:prod": "bun run build",
    "start:prod": "bun run build && cd ../app && python main.py",
    "setup": "bun install && bun run build",
    "docker:build": "cd .. && docker build -t doku-modern .",
    "docker:run": "docker run -d -v /var/run/docker.sock:/var/run/docker.sock:ro -v /:/hostroot:ro -p 9090:9090 doku-modern",
    "docker:dev": "cd .. && docker-compose -f docker-compose.dev.yml up",
    "clean": "rm -rf dist node_modules/.vite",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroui/react": "^2.8.2",
    "@heroui/table": "^2.2.24",
    "axios": "^1.6.8",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.363.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
